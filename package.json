{
  "name": "project-portal-ui",
  "version": "0.0.3",
  "scripts": {
    "dev": "next dev",
    "build": "npm run lint && next build",
    "start": "next start",
    "build-stats": "cross-env ANALYZE=true npm run build",
    "export": "next export",
    "build-prod": "run-s clean build export",
    "clean": "rimraf .next .swc out coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "next lint --fix && prettier '**/*.{json,yaml}' --write --ignore-path .gitignore",
    "check-types": "tsc --noEmit --pretty && tsc --project cypress --noEmit --pretty",
    "test": "vitest",
    "coverage": "vitest --coverage --run",
    "commit": "cz",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "e2e": "start-server-and-test dev http://localhost:3000 cypress",
    "e2e:headless": "start-server-and-test dev http://localhost:3000 cypress:headless",
    "prepare": "husky install",
    "postbuild": "next-sitemap",
    "ngrok": "ngrok http 3000"
  },
  "dependencies": {
    "@ag-grid-community/core": "^32.0.2",
    "@ag-grid-enterprise/column-tool-panel": "^32.0.2",
    "@ag-grid-enterprise/server-side-row-model": "^32.0.2",
    "@dnd-kit/core": "^6.1.0",
    "@next/env": "^14.2.5",
    "@react-google-maps/api": "^2.19.3",
    "@reduxjs/toolkit": "^2.2.7",
    "@stoplight/elements": "^8.3.4",
    "ag-grid-community": "^32.0.2",
    "ag-grid-enterprise": "^32.0.2",
    "ag-grid-react": "^32.0.2",
    "animate-css-grid": "^1.5.1",
    "axios": "^1.7.3",
    "cropperjs": "^1.6.2",
    "dotenv": "^16.4.5",
    "fuse.js": "^7.0.0",
    "html-react-parser": "^5.1.12",
    "intl-tel-input": "^24.3.0",
    "libphonenumber-js": "^1.11.5",
    "lodash": "^4.17.21",
    "next": "^14.2.5",
    "next-auth": "^4.24.7",
    "next-nprogress-bar": "^2.3.13",
    "next-sitemap": "^4.2.3",
    "planby": "^1.1.7",
    "rc-dropdown": "^4.2.0",
    "rc-slider": "^11.1.5",
    "react": "^18.3.1",
    "react-color": "^2.19.3",
    "react-cropper": "^2.3.3",
    "react-datepicker": "^7.3.0",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "react-toastify": "^10.0.5",
    "react-tooltip": "^5.28.0",
    "redux-persist": "^6.0.0",
    "swiper": "^11.1.9",
    "use-long-press": "^3.2.0",
    "usehooks-ts": "^3.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/cz-commitlint": "^19.2.0",
    "@eslint/compat": "^1.1.1",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.8.0",
    "@next/bundle-analyzer": "^14.2.5",
    "@percy/cli": "^1.29.3",
    "@percy/cypress": "^3.1.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@testing-library/cypress": "^10.0.2",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/eslint__js": "^8.42.3",
    "@types/google.maps": "^3.55.12",
    "@types/intl-tel-input": "^18.1.4",
    "@types/node": "^22.1.0",
    "@types/react": "^18.3.3",
    "@types/react-color": "^3.0.12",
    "@types/react-datepicker": "^7.0.0",
    "@types/swagger-ui-react": "^4.18.3",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^2.0.5",
    "autoprefixer": "^10.4.20",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cssnano": "^7.0.4",
    "cypress": "^13.13.2",
    "cypress-ssr-localhost-mocker": "^0.0.4",
    "eslint": "^9.8.0",
    "eslint-config-next": "^14.2.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^3.4.0",
    "eslint-plugin-jest": "^28.7.0",
    "eslint-plugin-jest-dom": "^5.4.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-testing-library": "^6.2.2",
    "eslint-plugin-unused-imports": "^4.0.1",
    "eslint-plugin-vitest": "^0.5.4",
    "http-server": "^14.1.1",
    "husky": "^9.1.4",
    "jsdom": "^25.0.0",
    "lint-staged": "^15.2.8",
    "msw": "^2.3.5",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.41",
    "prettier": "3.3.3",
    "rimraf": "^6.0.1",
    "sass": "^1.77.8",
    "semantic-release": "^24.0.0",
    "start-server-and-test": "^2.0.5",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.3.5",
    "vitest": "^2.0.5",
    "vitest-fetch-mock": "^0.3.0"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "overrides": {
    "eslint": "^9.8.0"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  },
  "importSort": {
    ".js, .jsx, .es6, .es": {
      "parser": "babylon",
      "style": "eslint",
      "options": {}
    },
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "eslint",
      "options": {}
    }
  }
}
